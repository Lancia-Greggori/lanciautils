{
	"name": "decancer",
	"manifest_version": 3,
	"version": "1.0",
	"browser_specific_settings": {
		"gecko": {
		  "id": "decancer@nodomain.com"
		}
	},
	"content_scripts": [
		{
			"js": ["hide.js"],
			"run_at": "document_start",
			"matches": ["<all_urls>"]
		}, {
			"js": ["decancer.js", "show.js"],
			"run_at": "document_end",
			"matches": ["<all_urls>"]
		}
	]
}
