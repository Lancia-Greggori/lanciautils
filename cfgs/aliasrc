alias sl=ls
alias l=ls
alias ls='ls --color'
alias la='ls -a'
alias ll='ls -l'
alias cp='cp -i'
alias mv='mv -i'

alias es-spell='hunspell -d es'
alias tcat='tar tvf'
alias info='info --vi-keys'
alias lynx='lynx -vikeys'
alias show-keysym='xev 2>&1 | grep -F keysym'
alias gdb='gdb -q'
alias lsuserps='ps -u $USER -o user,cmd'
